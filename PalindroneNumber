class Solution(object):
    def isPalindrome(self,n):
        temp=n
        res=0
        while n>0:
            rem=n%10
            res=res*10+rem
            n=n//10
        if temp==res:
            return True
        else:
            return False
n=121
solution=Solution()
res=solution.isPalindrome(n)
print(res)
